package com.kosta.exam;

public class MethodGetFactorial {

	public static int factorial(int number) {
		int sum = 0;
		for (int i = 0; i < number; i++) {
			sum += i;
		}
		return sum;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = factorial(3);
		System.out.println(a);
	}

}
