package com.kosta.exam;
public class MethodEqualsArray {
	public static boolean check(double []a, double []b) {
		boolean flag = false;
		
		if (a.length==b.length) {
			return flag;
		}else {
			for (int i= 0;  i< b.length; i++) {
				if (a[i]!=b[i]) {
					flag = false;
				}
			}
		}
		return flag;
	}
	public static void main(String[] args) {	
		System.out.println();
		System.out.println();
	}
}
