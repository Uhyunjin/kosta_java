package com.kosta.exam08;

public class Rect extends Shape {

	public Rect() {}
	public Rect(int x, int y) {
		super(x,y);
	}
	
	@Override
	public void calcArea() {
		area = x*y;
	}

	@Override
	public void calcRound() {
		round = (2*x)+(2*y);
	}
	@Override
	public String toString() {
		return "Rect [x=" + x + ", y=" + y + ", area=" + area + ", round=" + round + "]";
	}
	
}
