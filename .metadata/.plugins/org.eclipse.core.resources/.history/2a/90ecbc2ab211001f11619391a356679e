package com.kosta.exam;

public class MethodMaxArray {
	public static void maxArray(int []arr) {
		int max = arr[0];
		
		for (int i : arr) {
			if (i>arr[i-1]) {
				max = i;
			}
		}
		
		System.out.println(max);
	}
	
	public static void main(String[] args) {
		int []array = {1,2,3,4,5};
		maxArray(array);
	}

}
