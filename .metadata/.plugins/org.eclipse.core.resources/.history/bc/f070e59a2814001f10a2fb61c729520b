package com.kosta.exam31;

public class Point {
		int x;
		int y;
		
		@Override
		public String toString() {
			return "Point [x=" + x + ", y=" + y + "]";
		}
		public int getX() {
			return x;
		}
		public void setX(int xCoord) {
			this.x = xCoord;
		}
		public int getY() {
			return x;
		}
		public void setY(int yCoord) {
			this.y = yCoord;
		}
		
		Point(int xCoord, int yCoord){
			this.x = xCoord;
			this.y = yCoord;
		}
		Point(){}
	
}
