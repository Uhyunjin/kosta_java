package com.kosta.exam;

public class MethodMaxArray {
	public static void maxArray(int []arr) {
		int max = arr[0];
		
		for (int i = 0; i < arr.length; i++) {
			if (arr[i]>max) {
				max = arr[i];
			}
		}
		System.out.println(max);
	}
	
	public static void main(String[] args) {
		int []array = {1,2,3,4,5};
		maxArray(array);
	}

}
