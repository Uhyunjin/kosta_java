package com.kosta.exam30;

public class PlaenTest {

	public static void main(String[] args) {
		Plane p1 = new Plane("airbus", "A380", 500);
		Plane p2 = new Plane();
		p1.setProduction("boeing");
		p1.setModel("B787");
		p1.setMaxPax(200);
		
		p1.toString();
		p2.toString();
		
		p1.setPlanes();
		Plane.setPlanes();
		p2.setPlanes();
		Plane.setPlanes();
		
		
		System.out.println(Plane.getPlanes());

	}

}
