package com.kosta.exam;

public class MatrixTest {

	public static void main(String[] args) {
		int [][]a = new int[4][3];
		
		int n = 1;
		for (int i = 0; i < 4; i++) {
			for (int j = 0; j < 3; j++) {
				a[i][j] = n;
				n++;
				System.out.printf("%5d",a[i][j]);
			}
			System.out.println();
		}

	}

}
